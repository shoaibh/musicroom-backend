{
  "name": "server",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "deploy": "npm install && npm run build && npm run start",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js -d src/app/ormconfig.ts",
    "migrate:all": "npm run typeorm migration:run",
    "migrate:generate": "npm run typeorm migration:generate",
    "migrate:create": "npm run typeorm migration:create",
    "migrate:undo": "npm run typeorm migration:revert",
    "docker:dependencies:up": "docker-compose -f ./docker/docker_dependency.yml up -d",
    "docker:dependencies:down": "docker-compose -f ./docker/docker_dependency.yml down",
    "seed:fake": "ts-node src/db/seeds/fake.seed.runner.ts"
  },
  "engines": {
    "node": "12.16.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "dependencies": {
    "@hapi/joi": "^17.1.1",
    "@nestjs/common": "^10.2.10",
    "@nestjs/core": "^10.2.10",
    "@nestjs/mongoose": "^10.0.2",
    "@nestjs/platform-express": "^10.2.10",
    "@nestjs/platform-socket.io": "^10.2.10",
    "@nestjs/typeorm": "^10.0.1",
    "@nestjs/websockets": "^10.2.10",
    "axios": "^1.6.2",
    "bcrypt": "^5.1.1",
    "class-validator": "^0.14.0",
    "dotenv": "^16.3.1",
    "express-rate-limit": "^7.1.5",
    "faker": "^5.5.3",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "moment": "^2.29.4",
    "mongodb": "^5.9.2",
    "mongoose": "^8.0.3",
    "reflect-metadata": "^0.1.14",
    "rimraf": "^5.0.5",
    "socket.io": "^4.7.2",
    "ts-node": "^10.9.2",
    "tsc-watch": "^6.0.4",
    "typeorm": "^0.3.17",
    "typescript": "^5.3.3",
    "yt-search": "^2.10.4",
    "ytdl-core": "^4.11.5"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.2.1",
    "@nestjs/schematics": "^10.0.3",
    "@nestjs/testing": "^10.2.10",
    "@types/eslint": "^8.44.8",
    "@types/eslint-scope": "^3.7.7",
    "@types/estree": "^1.0.5",
    "@types/express": "^4.17.21",
    "@types/multer": "^1.4.11",
    "@types/supertest": "^2.0.16",
    "@types/yt-search": "^2.10.3",
    "@typescript-eslint/eslint-plugin": "^6.13.2",
    "@typescript-eslint/parser": "^6.13.2",
    "eslint": "^8.55.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.0.1",
    "prettier": "^3.1.0",
    "supertest": "^6.3.3",
    "ts-loader": "^9.5.1",
    "tsconfig-paths": "^4.2.0"
  }
}
